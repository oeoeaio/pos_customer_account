<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="account_payment_product" model="product.product">
            <field name="name">Account Payment</field>
            <field name="available_in_pos" eval="True" />
            <field name="taxes_id" eval="[(6, 0, [ref('l10n_au.1_au_tax_sale_exempt')])]"/>
        </record>

        <record id="pos_customer_account_journal" model="account.journal">
            <field name="name">Account</field>
            <field name="code">CACC</field>
            <field name="type">bank</field>
            <field name="sequence">10</field>
            <field name='company_id' ref="base.main_company"/>
            <field name="default_account_id" ref="l10n_au.1_au_11200"/>
        </record>
    </data>
</odoo>
